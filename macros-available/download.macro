# Download the same file(s) from many servers. --download=hostNameRegex,fullPathToFile ~ user

list ~!Global,download-0!~

# the internalIP will work if we are connected to a VPN
chooseFirst IP,externalIP,internalIP

stashResults Download,sourcelist

retrieveResults Download,sourcelist
toString mkdir -p %externalFQDN%
exec

retrieveResults Download,sourcelist
toString scp -r %IP%:~!Global,download-1!~ %externalFQDN%
exec
